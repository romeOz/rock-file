{
    "name": "romeoz/rock-file",
    "description": "File Manager module for Rock Framework",
    "keywords": [
        "filesystem", "file", "files", "storage", "dropbox", "aws",
        "s3", "ftp", "sftp", "remote", "webdav", "file systems", "cloud",
        "cloud files", "rackspace", "copy.com"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-file/issues",
        "source": "https://github.com/romeOz/rock-file"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
      "php": ">=5.4.0",
      "romeoz/rock-helpers" : "0.10.*",
      "romeoz/rock-events" : "0.10.*",
      "league/flysystem": "1.0.*",
      "league/flysystem-cached-adapter": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7.0"
    },
    "autoload": {
        "psr-4": {
            "rock\\file\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}